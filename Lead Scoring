   "cell_type": "code",
   "execution_count": 2,
   "metadata": {
    "execution": {
     "iopub.execute_input": "2025-02-17T00:55:54.286354Z",
     "iopub.status.busy": "2025-02-17T00:55:54.285994Z",
     "iopub.status.idle": "2025-02-17T00:55:54.295784Z",
     "shell.execute_reply": "2025-02-17T00:55:54.294734Z",
     "shell.execute_reply.started": "2025-02-17T00:55:54.286285Z"
    }
   },
   "outputs": [],
   "source": [
    "# UDF for EDA:\n",
    "\n",
    "# Function to see all percentiles for a numeric column\n",
    "def display_percentiles(data, column_name, percentiles_per_row=20, round_to=2):\n",
    "    # Check if the specified column is numeric\n",
    "    if not pd.api.types.is_numeric_dtype(data[column_name]):\n",
    "        raise ValueError(f\"The column '{column_name}' must be numeric.\")\n",
    "    \n",
    "    # Generate the list of percentiles from 1% to 100%\n",
    "    percentiles = [i for i in range(1, 101)]\n",
    "\n",
    "    # Calculate the percentiles using numpy\n",
    "    percentile_values = np.percentile(data[column_name], percentiles)\n",
    "\n",
    "    # Display the percentiles and their corresponding values in a clean format\n",
    "    for i in range(0, len(percentile_values), percentiles_per_row):\n",
    "        percentiles_batch = percentile_values[i:i+percentiles_per_row]\n",
    "        \n",
    "        # Adjust the range for last batch\n",
    "        end_idx = min(i + percentiles_per_row, len(percentile_values))\n",
    "        \n",
    "        # Format the output with string formatting\n",
    "        formatted_output = ', '.join([f\"{percentiles[j]}%: {percentiles_batch[idx]:.{round_to}f}\" \n",
    "                                     for idx, j in enumerate(range(i, end_idx))])\n",
    "        print(formatted_output)\n",
    "\n",
    "\n",
    "# Function to cap outliers on either end of a numeric variable.\n",
    "def cap_outliers(data, column_name, lower_pct=0.05, upper_pct=0.99):\n",
    "    \"\"\"\n",
    "    Cap the values of a column at the given percentiles for both the lower and upper bounds.\n",
    "    Use None to indicate that the top/bottom should not be capped.\n",
    "    \n",
    "    Parameters:\n",
    "    data (DataFrame): The DataFrame containing the column.\n",
    "    column_name (str): The column to be capped.\n",
    "    lower_pct (float or None): The percentile for the lower bound (default is 0.05 or 5th percentile).\n",
    "    upper_pct (float or None): The percentile for the upper bound (default is 0.99 or 99th percentile).\n",
    "    \n",
    "    Returns:\n",
    "    pd.Series: The capped column with outliers handled.\n",
    "    \"\"\"\n",
    "    # Calculate percentiles only if they are not None\n",
    "    if lower_pct is not None:\n",
    "        lower_val = data[column_name].quantile(lower_pct)\n",
    "        data[column_name] = data[column_name].clip(lower=lower_val)\n",
    "    \n",
    "    if upper_pct is not None:\n",
    "        upper_val = data[column_name].quantile(upper_pct)\n",
    "        data[column_name] = data[column_name].clip(upper=upper_val)\n",
    "    \n",
    "    return data[column_name]"
   ]
  },
