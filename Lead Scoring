   "source": [
    "# Get frequencies of each Lead Source and pct of total\n",
    "lead_source_freq = data['Lead Source'].value_counts().reset_index(name='counts')\n",
    "lead_source_freq.columns = ['Lead Source', 'counts']  # Fix column name issues\n",
    "lead_source_freq['pct_of_total'] = round((lead_source_freq['counts'] / len(data)), 4)\n",
    "\n",
    "# Calculate the percentage of 0's and 1's in \"Converted\" per \"Lead Source\"\n",
    "conversion_percentages_source = data.groupby('Lead Source')['Converted'].value_counts(normalize=True).round(4).unstack(fill_value=0).reset_index()\n",
    "conversion_percentages_source.rename(columns={0: 'converted_0_pct', 1: 'converted_1_pct'}, inplace=True) # rename cols\n",
    "\n",
    "# Combine outputs\n",
    "combined_data_source = pd.merge(lead_source_freq, conversion_percentages_source, on='Lead Source', how='left')\n",
    "combined_data_source"
   ]
  },
