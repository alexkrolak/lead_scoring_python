    "\n",
    "# Plot countplot with ordered categories\n",
    "sns.countplot(data=data, x='Asymmetrique Activity Index', ax=axs[0, 0], order=activity_index_order)\n",
    "sns.boxplot(data=data, x='Asymmetrique Activity Score', ax=axs[0, 1])\n",
    "sns.countplot(data=data, x='Asymmetrique Profile Index', ax=axs[1, 0], order=profile_index_order)\n",
